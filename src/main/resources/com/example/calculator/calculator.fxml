            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="threeButton" layoutX="112.0" layoutY="109.0" mnemonicParsing="false" onAction="#threeFunc"
              prefHeight="40.0" prefWidth="37.0" text="3">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="fourButton" layoutX="157.0" layoutY="109.0" mnemonicParsing="false" onAction="#fourFunc"
              prefHeight="40.0" prefWidth="37.0" text="4">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="fiveButton" layoutX="18.0" layoutY="160.0" mnemonicParsing="false" onAction="#fiveFunc"
              prefHeight="40.0" prefWidth="37.0" text="5">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="sixButton" layoutX="65.0" layoutY="160.0" mnemonicParsing="false" onAction="#sixFunc"
