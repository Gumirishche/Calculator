              text="reset">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="pointButton" layoutX="157.0" layoutY="209.0" mnemonicParsing="false" onAction="#pointFunc"
              prefHeight="40.0" prefWidth="37.0" text=".">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="zeroButton" layoutX="65.0" layoutY="209.0" mnemonicParsing="false" onAction="#zeroFunc"
              prefHeight="40.0" prefWidth="37.0" text="0">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
   </AnchorPane>
</VBox>
