            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="degreeButton" layoutX="296.0" layoutY="109.0" mnemonicParsing="false" onAction="#degreeFunc"
              prefHeight="40.0" prefWidth="37.0" text="^">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="oneButton" layoutX="18.0" layoutY="109.0" mnemonicParsing="false" onAction="#oneFunc"
              prefHeight="40.0" prefWidth="37.0" text="1">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="twoButton" layoutX="65.0" layoutY="109.0" mnemonicParsing="false" onAction="#twoFunc"
              prefHeight="40.0" prefWidth="37.0" text="2">
