         <font>
            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="eightButton" layoutX="157.0" layoutY="160.0" mnemonicParsing="false" onAction="#eightFunc"
              prefHeight="40.0" prefWidth="37.0" text="8">
