<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="293.0" prefWidth="392.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.calculator.CalculatorController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <AnchorPane prefHeight="356.0" prefWidth="365.0">
      <TextField fx:id="calculatorText" alignment="CENTER_RIGHT" layoutX="10.0" layoutY="5.0" onAction="#calculatorFunc"
                 prefHeight="49.0" prefWidth="334.0">
         <font>
            <Font size="18.0"/>
         </font>
      </TextField>
      <Button fx:id="equallyButton" layoutX="217.0" layoutY="209.0" maxWidth="123.0" mnemonicParsing="false"
              onAction="#equallyFunc" prefHeight="40.0" prefWidth="63.0" text="=">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="sumButton" layoutX="218.0" layoutY="109.0" mnemonicParsing="false" onAction="#sumFunc"
              prefHeight="40.0" prefWidth="63.0" text="+">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="minusButton" layoutX="296.0" layoutY="209.0" mnemonicParsing="false" onAction="#minusFunc"
              prefHeight="40.0" prefWidth="37.0" text="-">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="divButton" layoutX="296.0" layoutY="160.0" mnemonicParsing="false" onAction="#divFunc"
